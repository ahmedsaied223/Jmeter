        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Set variables per thread">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.util.Random
import java.math.BigInteger
import java.security.SecureRandom

// Create a SecureRandom instance
def random = new SecureRandom()

// Generate a random number unique to each thread
def randomNumber = BigInteger.valueOf(random.nextLong()).abs() % BigInteger.TEN.pow(19)
def randomNumberString = randomNumber.toString()
// Check if the random number has fewer than 19 digits
if (randomNumberString.length() &lt; 19) {
    log.info(&quot;random number : &quot; + randomNumberString + &quot; has less than 19 digits&quot;)

    // Calculate the number of digits to append
    def digitsToAppend = 19 - randomNumberString.length()

    // Generate additional random digits to append
    def additionalDigits = new Random().nextInt(9 * (int)Math.pow(10, digitsToAppend - 1))

    // Append the additional random digits
    randomNumberString += String.format(&quot;%0${digitsToAppend}d&quot;, additionalDigits)
}

// Convert the random number back to a BigInteger
randomNumber = new BigInteger(randomNumberString)

// Store these as Meter variable unique to each thread
vars.put(&quot;STREAM_ID&quot;, randomNumber.toString())
vars.put(&quot;STREAM_NAME&quot;, &quot;SRC_NAT_OOMSports_NHL_COL_K809_&quot;+randomNumber.toString())

def chars = (&apos;a&apos;..&apos;z&apos;) + (&apos;A&apos;..&apos;Z&apos;) + (&apos;0&apos;..&apos;9&apos;)
def signalID = (0..24).collect { chars[random.nextInt(chars.size())] }.join()
vars.put(&quot;SIGNAL_ID&quot;, signalID.toString())

</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
