              <stringProp name="parameters"></stringProp>
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">import org.apache.commons.codec.binary.Base64;

// Get the extracted value from the XPath Extractor
String supplementalPropertyValue = vars.get(&quot;supplementalPropertyValue&quot;);

// Decode the base64 encoded value
byte[] decodedBytes = Base64.decodeBase64(supplementalPropertyValue);

// Convert the decoded bytes to a String
String decodedString = new String(decodedBytes);

// Print the signal value to JMeter log
//log.info(&quot;Decoded value: &quot; + decodedString);

// Set the signal value as a JMeter variable for further use if needed
vars.put(&quot;DECODED_SIGNAL_JSON&quot;, decodedString);

</stringProp>
